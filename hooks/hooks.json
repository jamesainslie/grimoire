{
  "description": "Grimoire plugin hooks - builds binaries on first session start",
  "hooks": {
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bash ${CLAUDE_PLUGIN_ROOT}/hooks/scripts/setup.sh",
            "timeout": 120
          }
        ]
      }
    ]
  }
}
